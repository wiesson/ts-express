{
  "name": "ts-express",
  "version": "1.0.0",
  "main": "start.js",
  "license": "MIT",
  "devDependencies": {
    "@types/cors": "^2.8.3",
    "@types/jest": "^22.0.1",
    "@types/node": "^9.3.0",
    "@types/supertest": "^2.0.4",
    "jest": "^22.1.1",
    "nodemon": "^1.14.11",
    "prettier": "^1.10.2",
    "supertest": "^3.0.0",
    "ts-jest": "^22.0.1",
    "ts-node": "^4.1.0",
    "tslint": "^5.9.1"
  },
  "dependencies": {
    "@types/compression": "^0.0.35",
    "@types/express": "^4.11.0",
    "@types/morgan": "^1.7.35",
    "compression": "^1.7.1",
    "cors": "^2.8.4",
    "express": "^4.16.2",
    "morgan": "^1.9.0",
    "typescript": "^2.6.2",
    "zeromq": "^4.6.0"
  },
  "scripts": {
    "start": "node index.js",
    "prestart:prod": "tsc",
    "start:prod": "node dist/server.js",
    "format": "./node_modules/.bin/prettier --write --single-quote --trailing-comma es5 './?(src|test)/**/*.ts'",
    "test": "./node_modules/.bin/jest --forceExit",
    "dev": "nodemon --ext js,ts --watch 'src' start.js"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "./node_modules/ts-jest/preprocessor.js"
    },
    "testMatch": [
      "**/test/**/*.test.(ts|js)"
    ],
    "testEnvironment": "node"
  }
}
